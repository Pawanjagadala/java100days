package java100days;
import java.util.*;
public class spyNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the number");
		int n=sc.nextInt();
		int sum=0;
		int product=1;
		int num=n;
		while(num>0)
		{
			int rem;
			rem=num%10;
			sum=sum+rem;
			product=product*rem;
			num=num/10;
		}
		if(sum==product)
		{
			System.out.println("spy number");
		}
	}

}
