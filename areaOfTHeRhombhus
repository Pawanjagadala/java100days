package java100days;
import java.util.*;
public class areaofrhombus {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the diagonla valuues");
		int d1=sc.nextInt();
		int d2=sc.nextInt();
